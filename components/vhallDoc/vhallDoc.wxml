<view style="display:{{switchStatus? 'block' : 'none'}}" id="vhallDoc">
  <view
    class="view"
    wx:for="{{vhallDoc}}"
    wx:key="{{index}}"
    id="view-{{item.cid}}"
    wx:if="{{item.active}}"
    style="height:{{item.vh}}px;"
  >
    <image
      src="{{item.imageUrl}}"
      wx:if="{{item.imageUrl}}"
      style="width:{{item.imgVw}}px;height:{{item.imgVh}}px;"
      class="image"
    />
    <canvas
      canvas-id="{{item.cid}}"
      class="canvas"
      bindtouchstart="_touchstart"
      bindtouchmove="_touchmove"
      bindtouchend="_touchend"
      disable-scroll="true"
    ></canvas>
  </view>
</view>
